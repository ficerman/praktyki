const arr1 = ["Anna", "Maria"]
const arr2 = ["Adam", "Marek", "Bartek", "Tomek"]
const numbers = [1, 2, 3, 4, 5]

const concatArray = (arr1, arr2) => arr1.concat(arr2)
console.log(concatArray(arr1, arr2))

const lastElementInArray = arr => arr.reverse()[0]
console.log(lastElementInArray(arr2))

const filterArray = (arr, arg) => arr.filter(number => number == arg)
console.log(filterArray(numbers, 1))

const sumArray = arr => arr.reduce((sum, currentValue) => sum + currentValue, 0)
console.log(sumArray(numbers))

const mapArray = arr => arr.map(num => Math.pow(num, 2) + 3)
console.log(mapArray(numbers))
